# Pattern Regular Expression for validation
pattern.regex.password = ^(?=.*[a-z0-9])(?=.*[A-Z])(?=.*[!@#$%])[0-9a-zA-Z!@#$%]*$
pattern.password =^(?=.*[0-9])(?=.*[A-Z])(?=.*[!@#$%])[0-9a-zA-Z!@#$%]*$
pattern.regex.email =^[_A-Za-z0-9]+(\\.[_A-Za-z0-9]+)*@[A-Za-z0-9]+(\\.[A-Za-z0-9]+)*(\\.[A-Za-z]{2,})$
pattern.regex.phone =^[0-9]{9,12}$
pattern.regex.identity.number =^[0-9]{9,15}$
pattern.regex.fraction.digits =##,###,##0.00
pattern.regex.cvv = ^[0-9]{3,4}$
pattern.regex.credit.card = ^4[0-9]{12}(?:[0-9]{3})?$
pattern.regex.double.number = \\d{1,22}(\\.\\d{1,3})?
pattern.regex.name = ^[a-zA-Z]{1,10}$
pattern.regex.zipcode = ^(?=.{4,6}$)(?!.* {2,})[a-zA-Z0-9][a-zA-Z0-9 ]*[a-zA-Z0-9]$
pattern.regex.date = ^\d{4}[-/.]\d{1,2}[-/.]\d{1,2}$
