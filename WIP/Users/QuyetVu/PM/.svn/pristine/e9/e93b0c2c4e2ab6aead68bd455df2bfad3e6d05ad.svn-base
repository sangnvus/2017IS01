<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">
<f:view locale="#{languageBean.locale}">
	<h:head>
		<h:outputScript library="js" name="main.js" />
		<title><ui:insert name="title">Sàn giao dịch vận tải</ui:insert></title>
		<link rel="shortcut icon" type="image/x-icon"
			href="#{request.contextPath}/resources/images/logo-s.png" />
	</h:head>
	<h:body>
		<h:outputStylesheet library="css" name="main.css" />
		<div
			style="border-width: 0px; border-color: green; border-style: solid;">
			<ui:insert name="header">
				<ui:include src="MainHeader.xhtml" />
			</ui:insert>
		</div>
		<div
			style="border-width: 0px; border-color: black; border-style: solid;">
			<ui:insert name="content">
				<ui:include src="LoginPageContent.xhtml"/>
			</ui:insert>
		</div>
		<div class="footer">
			<ui:insert name="footer">
				<ui:include src="MainFooter.xhtml" />
			</ui:insert>
		</div>
	</h:body>
</f:view>
</html>