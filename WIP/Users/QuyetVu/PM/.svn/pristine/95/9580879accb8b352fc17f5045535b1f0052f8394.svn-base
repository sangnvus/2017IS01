body,ul,ol,form,h1,h2,h3,h4,h5,h6,fieldset,blockquote,p {
	padding: 0;
	margin: 0;
	list-style-type: none;
	border: 0;
	font-size: 100%;
}

body {
	background: #FFFFFF;
	min-height: 100%;
	height: auto !important;
	height: 100%;
	font: 12px 'Arial';
	position: relative;
	color: #000000;
}

#over {
	display: none;
	background: #000;
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	opacity: 0.8;
	z-index: 999;
}

/*///////////////////////////////Footer-Begin///////////////////////////////////////////*/
footer {
	width: 100%;
	bottom: 0px;
	left: 0px;
	background: #0860CA url(../images/bg-footer.png) no-repeat left center;
	z-index: 6;
	height: 77px;
	position: relative;
}

footer .menu-footer {
	width: 100%;
	padding: 20px 0 15px 0;
}

footer .menu-footer ul {
	background: url(../images/menu-footer.png) no-repeat left
		center;
	width: 271px;
	height: 24px;
	margin: 0px auto;
	text-align: center;
	padding: 5px 0;
	
}

footer .menu-footer ul li {
	display: inline;
	padding: 0 5px;
	border-left: 1px solid #fff;
