/*///////////////////////////////////BUTTON/////////////////////////////*/
.button {
	color: #FFFFFF;
	font-weight: bold;
	display: inline-block;
	text-decoration: none;
	border: 0;
	outline: 0;
	padding: 10px 40px;
	background: #0082bb;
	border-right: 5px solid #20affe;
	/* box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.5); */
	position: relative;
	text-transform: uppercase;
}

.button:before,.button:after {
	border-right: 2px solid;
	content: '';
	display: block;
	height: 8px;
	margin-top: -6px;
	position: absolute;
	transform: rotate(135deg);
	right: 10px;
	top: 50%;
	width: 0;
}

.button:after {
	margin-top: -1px;
	-moz-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	transform: rotate(45deg);
}

.button:hover {
	background: #252525;
	border-right: 5px solid #252525;
}

.button1 {
	color: #FFFFFF;
	width: 100%;
	font-weight: bold;
	display: inline-block;
	text-decoration: none;
	border: 0;
	outline: 0;
	margin: 0 1px 10px 0;
	padding: 10px 0px 10px 0px;
	background: #0082bb;
	box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.5);
	/* border-radius: 5px; */
	position: relative;
	right: 0px;
	bottom: 0px;
}

.button1:before,.button1:after {
	border-right: 2px solid;
	content: '';
	display: block;
	height: 8px;
	margin-top: -6px;
	position: absolute;
	/*  -moz-transform: rotate(135deg);
    -o-transform: rotate(135deg); */
	/*  -webkit-transform: rotate(135deg); */
	transform: rotate(135deg);
	right: 10px;
	top: 50%;
	width: 0;
}

.button1:after {
	margin-top: -1px;
	-moz-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	transform: rotate(45deg);
}

.button1:hover {
	background: #252525;
}

