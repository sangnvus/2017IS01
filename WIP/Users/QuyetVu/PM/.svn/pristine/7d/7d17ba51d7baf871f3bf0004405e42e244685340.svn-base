body, ul, ol, form, h1, h2, h3, h4, h5, h6, fieldset, blockquote, p {
	padding: 0;
	margin: 0;
	list-style-type: none;
	border: 0;
	font-size: 100%;
}

body {
	background: #FFFFFF; /*#279DFF; /*#070716;*/
	min-height: 100%;
	height: auto !important;
	height: 100%;
	font: 12px 'Arial';
	position: relative;
	color: #000000; /*#fff; */
}

.ui-link {
	margin: 0px !important;
}

.ui-button {
	margin-right: 0px !important;
}

.ui-widget {
	font-size: 12px !important;
	/* font-weight: normal; */
}
/* table.infos {
	width: 100%;
	table-layout: auto;
} */
table.infos tr>:nth-child(odd) {
	text-align: right;
}

table.infos2 tr>:nth-child(odd) {
	text-align: left;
}

.ui-datatable table {
	table-layout: auto;
}

.ui-link {
	color: #0073ea;
}

.ui-panel {
	margin-bottom: 5px;
}

.ui-outputlabel {
	font-size: 12px;
}

.ui-selectonemenu {
	font-size: 12px;
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	font-size: 12px;
	font-weight: normal;
}

.ui-column-title {
	font-size: 12px;
}

.ui-paginator .ui-paginator-current, .ui-paginator .ui-paginator-rpp-options
	{
	font-size: 12px;
}

.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	box-shadow: inset 0 0px 0px #8f8f8f !important;
}

